{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run main.exe (Compile then Run, No Debug)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/output/main.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LANG",
          "value": "zh_CN.UTF-8"
        },
        {
          "name": "LC_ALL",
          "value": "zh_CN.UTF-8"
        },
        {
          "name": "MSYS_UTF8",
          "value": "1"
        },
        {
          "name": "CONSOLE_ENCODING",
          "value": "UTF-8"
        }
      ],
      "externalConsole": true,
      "preLaunchTask": "build all"
    },

      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/MY_CODING_PROGRAM/C/groupchinesechess",
      "program": "d:/MY_CODING_PROGRAM/C/groupchinesechess/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
